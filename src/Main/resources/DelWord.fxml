<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="273.0" prefWidth="430.0"
            style="-fx-background-color: #009688;"
            xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="AddWordController">
   <Text layoutX="157.0" layoutY="33.0"
         strokeType="OUTSIDE" strokeWidth="0.0"
         text="DELETE Word" wrappingWidth="115.333984375">
      <font>
         <Font size="18.0" />
      </font>
   </Text>
   <TextField fx:id="deleteTarget" layoutX="14.0" layoutY="68.0"
              onAction="#searchWordDelete" onKeyReleased="#searchWordDelete"
              prefHeight="35.0" prefWidth="280.0" promptText="Target">
      <font>
         <Font size="14.0" />
      </font>
   </TextField>
   <TextField fx:id="deleteExplain" alignment="TOP_LEFT"
              layoutX="14.0" layoutY="126.0"
              prefHeight="130.0" prefWidth="280.0" promptText="Explain">
      <font>
         <Font size="14.0" />
      </font>
   </TextField>
   <Button layoutX="336.0" layoutY="68.0"
           mnemonicParsing="false" onAction="#deleteWordButton"
           prefHeight="35.0" prefWidth="64.0"
           styleClass="search" stylesheets="@styles.css" text="DELETE" />
   <Button layoutX="336.0" layoutY="221.0"
           mnemonicParsing="false" onAction="#goBack"
           prefHeight="35.0" prefWidth="64.0"
           styleClass="search" stylesheets="@styles.css" text="BACK" />
   <Text fx:id="alertDELETE" fill="RED" layoutX="316.0" layoutY="142.0"
         strokeType="OUTSIDE" strokeWidth="0.0" textAlignment="CENTER" wrappingWidth="104.13671875">
      <font>
         <Font size="14.0" />
      </font>
   </Text>
</AnchorPane>
